# top most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = crlf
trim_trailing_whitespace = true

[*.cs]
indent_style = tab
indent_size = tab
tab_size = 4
dotnet_sort_system_directives_first = false
dotnet_separate_import_directive_groups = false
#disable_formatter=true
max_line_length = 80
